{
  "name": "pokemon-card",
  "version": "1.0.0",
  "description": "My spectacular Nuxt.js project",
  "author": "",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/local.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "generate:error": "cross-env NODE_ENV=production GENERATE_ERROR_PAGE=true nuxt generate",
    "sls": "sls",
    "sls:deploy": "sls deploy",
    "deploy": "cross-env NODE_ENV=production ENDPOINT_ENV=cf node bin/deploy/index.js",
    "deploy:api_gw": "cross-env NODE_ENV=production ENDPOINT_ENV=api_gw run-s build sls:deploy",
    "delete": "sls remove"
  },
  "dependencies": {
    "@nuxtjs/dotenv": "^1.4.0",
    "aws-serverless-express": "^3.3.6",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "npm-run-all": "^4.1.5",
    "nuxt": "^2.0.0",
    "@types/cross-spawn": "^6.0.0",
    "@types/node": "^12.0.10",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "cross-spawn": "^6.0.5",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "nodemon": "^1.18.9",
    "aws-sdk": "^2.480.0",
    "consola": "^2.9.0",
    "dayjs": "^1.8.14",
    "minimatch": "^3.0.4"
  }
}
